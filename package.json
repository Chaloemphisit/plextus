{
  "name": "plextus",
  "version": "0.1.0",
  "description": "Pixel art editor designed to make tiled maps and tilesets.",
  "keywords": [
    "react",
    "webpack",
    "typescript",
    "babel",
    "sass",
    "hmr",
    "starter",
    "boilerplate"
